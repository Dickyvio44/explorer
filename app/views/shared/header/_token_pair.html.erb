<% content_for :title, t( "tabs.#{controller_name}.#{params[:action]}.title", network: @network[:name],
                          token1: @token1, token2: @token2,
                          token1symbol: @token1symbol, token2symbol: @token2symbol,
                          token1name: @token1name, token2name: @token2name) %>

<% content_for :description, t( "tabs.#{controller_name}.#{params[:action]}.description", network: @network[:name],
                                token1: @token1, token2: @token2,
                                token1symbol: @token1symbol, token2symbol: @token2symbol,
                                token1name: @token1name, token2name: @token2name) %>
<% content_for :keywords, t( "tabs.#{controller_name}.#{params[:action]}.keywords", network: @network[:name],
                             token1: @token1, token2: @token2,
                             token1symbol: @token1symbol, token2symbol: @token2symbol,
                             token1name: @token1name, token2name: @token2name) %>
<%= render partial: 'shared/title', locals: {
          icon: t("tabs.#{controller_name}.icon"),
          header: t( "tabs.#{controller_name}.header", network: @network[:name],
                     token1symbol: @token1symbol, token2symbol: @token2symbol,
                     token1name: @token1name, token2name: @token2name),
          reportrange:(params[:action] == 'trading_view' ? false : true)} %>