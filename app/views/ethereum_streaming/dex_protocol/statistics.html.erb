<div class='row mb-4'>
  <div class='col-12 col-xl-6'>
    <div class='card' id='TokenTrades'></div>
  </div>
  <div class='col-12 col-xl-6'>
    <div class='card' id='TradedCurrenciesByTime'></div>
  </div>
</div>


<script>
    class TokenTrades extends TimeChartComponent {
        configuration() {
            return {
                topElement: response => response.EVM.DEXTrades,
                chainId: response => response.EVM.DEXTrades[0].ChainId,
                title: () => `Token Trades by time`,
                id: 'TokenTrades',
                columns: [
                    {
                        name: 'Date', cell: row => row.Block.Date
                    },
                    {
                        name: 'Trades', cell: row => +row.count
                    }
                ],
                options: {
                    height: 300,
                    colors: ['28A745'],
                }
            }
        }
    }

    class TradedCurrenciesByTime extends TimeChartComponent {
        configuration() {
            return {
                topElement: response => response.EVM.DEXTrades,
                chainId: response => response.EVM.DEXTrades[0].ChainId,
                title: () => `Traded currencies by time`,
                id: 'TradedCurrenciesByTime',
                columns: [
                    {
                        name: 'Date', cell: row => row.Block.Date
                    },
                    {
                        name: 'Currencies', cell: row => +row.count
                    }
                ],
                options: {
                    height: 300,
                    colors: ['FFC106'],
                    legend: {position: 'top'},
                }
            }
        }
    }

    const variables = {
        network: '<%= @streaming%>',
        protocol: '<%= @protocol%>',
    };
    renderWithTime(variables, <%= limited_date_range_limit(@from, @till).join(',').html_safe %>, function f(variables) {
        renderComponent('<%=@streaming_access_token %>', [[TokenTrades, '#TokenTrades']], 'Token-Trades_1_1_1_1_1_1', variables);
        renderComponent('<%=@streaming_access_token %>', [[TradedCurrenciesByTime, '#TradedCurrenciesByTime']], 'Traded-currencies-by-time_1_1', variables);
    });
</script>
