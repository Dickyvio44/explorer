<div class="row">
  <div class="col-12 col-lg-12 mb-4">
    <div class="card" id="TransactionAttributes"></div>
  </div>
</div>
<script >
  class TransactionAttributes extends BootstrapVerticalTableComponent {
    configuration() {
      return  {
        topElement: function (response){ return response.EVM},
        chainId: function (response){ return response.EVM.Transactions[0].ChainId},
        columns: [
          {
            name: 'Block', cell: function(row){ return row.Transactions.Block.Number},  rendering: renderBlockLink,
          },
          {
            name: 'Timestamp', cell: function(row){ return row.Transactions.Block.Time},  rendering: renderDate,
          },
          {
            name: 'Sender', cell: function(row){ return row.Transactions.Transaction.From},rendering: renderAddressLink,
          },
          {
            name: 'Receiver', cell: function(row){ return row.Transactions.Transaction.To},rendering: renderAddressLink,
          },
          {
            name: 'Amount', cell: function(row){ return row.Transfers.Transfer.Amount},
          },
          {
            name: 'Gas Cost', cell: function(row){ return row.Transactions.Fee.SenderFee},
          },
          {
            name: 'Currency', cell: function(row){ return row.Transfers.Transfer.Currency.Symbol},
          },
          {
            name: 'Gas Used', cell: function(row){ return row.Transactions.Block.GasUsed},
          },
          {
            name: 'Success?', cell: function(row){ return row.Transfers.TransactionStatus.Success},
          },
        ]
      }
    }
  }
  const variables = {
    // network:'<%= @streaming%>',
    // hash:' <%= @hash%>',
    network:'eth',
  "hash": "0x42c84ae92884bfec73f9c04a75daee32c76ac9d72b98ee48f5b9b59b61afa7ae"

  }
    renderComponent(TransactionAttributes, "#TransactionAttributes",'Transaction-Attributes',variables);
</script>
