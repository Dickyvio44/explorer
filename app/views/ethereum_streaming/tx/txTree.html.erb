    class EventsAndCallsByTXHash extends TreeComponent {
        configuration() {
            return {
                topElement: function (response) {
                    return response.EVM
                },
                chainId: function (response) {
                    return response?.EVM?.Calls[0].ChainId
                },
                rendering: {renderJustAddressLink, renderSenderRecieverIcon, renderMethodLink, renderEventLink}
            }
        }
    }

    const variables = {
        network: '<%= @streaming%>',
        tx: '<%= @hash %>',
        // network:"eth",
        // tx:'0xc2685aa55d1365a75100eb8efcc44ddeb031dab1c7ad083775a8bd681d54d223'
    }

    renderComponent(EventsAndCallsByTXHash, '#EventsAndCallsByTXHash', 'Events-and-Calls_4', variables);
